pipeline {
    agent {
        label 'ubuntuVM'
    }

    tools {
        // Install the Maven version configured as "M3" and add it to the path.
        maven "maven 3.8.5"
    }

    stages {
        stage('Initialize'){
            steps{
                sh "mvn --version"
            }
        }
        stage('Clone code from VCS') {
            steps {
                git branch: 'main',
                credentialsId: 'itachi44',
                url: 'git@github.com:itachi44/CI-CD-Project.git'
                sh "ls -lat"
            }
        }
    }
}

